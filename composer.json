{
    "name": "anchor/laravel",
    "autoload": {
        "psr-4": {
            "Anchor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "AnchorTest\\": "tests"
        }
      },
      "scripts": {
        "test": "vendor/bin/phpunit"
      },
    "authors": [
        {
            "name": "Qazeem Abiodun",
            "email": "qudusqazeem@gmail.com"
        }
    ],
    "require": {
       "php": "^7.2.5|^8.0",
        "illuminate/support": "^10.0",
        "guzzlehttp/guzzle": "^7.2",
        "ext-json": "*"
      },
      "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^8.14",
        "phpunit/phpunit": "^9.6 || ^10.1",
        "illuminate/contracts": "^10.0",
        "illuminate/filesystem": "^10.0",
        "illuminate/database": "^10.0",
        "phpunit/php-code-coverage": "^9.2.13"
      },
      "description": "Anchor Laravel SDK",
      "keywords": [
        "php",
        "github",
        "laravel",
        "Open Source",
        "payments",
        "anchor",
        "thepeer",
        "laravel 6",
        "laravel 7",
        "laravel 8"
      ],
      "extra": {
        "laravel": {
            "providers": [
                "Anchor\\Providers\\AnchorProvider"
            ],
            "aliases": {
                "AnchorFacade": "Anchor\\Facades\\AnchorFacade"
            }
        }
    }
}
